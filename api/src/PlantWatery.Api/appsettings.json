{
    "DetailedErrors": true,
    "Ports": {
        "Main": 5011,
        "Metrics": 5022
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Enrichers.Span"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "Microsoft.AspNetCore": "Information",
                "Microsoft.AspNetCore.Hosting": "Information",
                "Microsoft.AspNetCore.Mvc": "Information",
                "Microsoft.AspNetCore.Routing": "Information",
                "System.Net.Http.HttpClient": "Information",
                "Infrastructure.Persistence": "Debug"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
                }
            }
        ],
        "Enrich": [
            "WithSpan",
            "FromLogContext",
            "WithMachineName",
            "WithEnvironmentName",
            "WithProcessId",
            "WithThreadId"
        ],
        "Properties": {
            "Application": "PlantWatery.Api",
            "Version": "1.0.0"
        }
    },
    "AllowedHosts": "*",
    "Auth": {
        "Authority": "",
        "Audience": "",
        "Issuer": ""
    },
    "CorsPolicy": {
        "AllowedOrigins": []
    }
}
